snapshots_to_delete=$(aws ec2 describe-snapshots --owner-ids 910456639623 --query 'Snapshots[?StartTime>=`2017-02-15`].SnapshotId' --output text)
echo "List of snapshots to delete: $snapshots_to_delete"

# actual deletion
echo "There are approximately $(echo $snaps_to_delete | wc -w) snapshots to delete..."
for snap in $snapshots_to_delete; do
  echo "Deleting $snap..."
  aws ec2 delete-snapshot --snapshot-id $snap
done
